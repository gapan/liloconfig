msgstr "Consola \"frame buffer\", 800x600x64k"

# File: 32-liloconfig, line: 191
# File: 64-liloconfig, line: 191
#: src/liloconfig:199
msgid "Frame buffer console, 1024x768x64k"
msgstr "Consola \"frame buffer\", 1024x768x64k"

# File: 32-liloconfig, line: 190
# File: 64-liloconfig, line: 190
#: src/liloconfig:200
#, fuzzy
msgid "Frame buffer console, 1280x1024x64k"
msgstr "Consola \"frame buffer\", 800x600x64k"

# File: 32-liloconfig, line: 190
# File: 64-liloconfig, line: 190
#: src/liloconfig:201
#, fuzzy
